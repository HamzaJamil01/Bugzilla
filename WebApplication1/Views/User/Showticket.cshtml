@model Ticket
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Description</th>
            <th>Assigned To</th>
            <th>Sprint No.</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>

            <tr>
                <td>@Model.Id</td>
                <td>@Model.Title</td>
                <td>@Model.Description</td>
                <td>@Model.AssignedTo</td>
                <td>@Model.sprint</td>
                <td>@Model.status</td>
            </tr>
    </tbody>
</table>